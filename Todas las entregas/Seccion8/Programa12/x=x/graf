set terminal postscript eps enhanced color solid "Arial" 12
set output "Grafi_Der_Sen.ps"
set grid xtics
set grid ytics
set xlabel "valores de x"
set ylabel "Derivada de sen(x)"
set xtics 0.05
set ytics 0.08
plot "datosderivadas" u 1:2 pt 10 title "cos(x)", "datosderivadas" u 1:3 pt 18 title "derivada calculada"
set output
set terminal pop
